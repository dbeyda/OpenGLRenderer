#shader vertex#version 410 corelayout(location = 0) in vec4 position;layout(location = 1) in vec4 normalColor;out vec4 fragmentColor;uniform mat4 u_MVP;void main(){    gl_Position = u_MVP * position;    fragmentColor = vec4(1, 1, 1, 0.9);};#shader fragment#version 410 corein vec4 fragmentColor;layout(location = 0) out vec4 color;void main(){    color = fragmentColor;};